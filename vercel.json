{
    "version":2,
    "builds": [
      {
        "src": "accountiko/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb","runtime" :"python3.9"}
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": { "distDir": "staticfiles_build"}
     
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "accountiko/wsgi.py"
      },
      {
        "src": "/static(.*)",
        "dest": "static/$1"
      }
    ]
  }